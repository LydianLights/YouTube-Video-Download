<Window x:Class="YouTubeVideoDownload.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YouTubeVideoDownload"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="textBoxVideoURL" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Video URL" VerticalAlignment="Top" Width="384"/>
        <Button x:Name="buttonGetVideo" Content="Get!" HorizontalAlignment="Left" Margin="420,8,0,0" VerticalAlignment="Top" Width="87" Click="buttonGetVideo_Click"/>

    </Grid>
</Window>
